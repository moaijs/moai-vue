{
  "name": "@moai-vue/core",
  "version": "0.0.1",
  "description": "Moai Vue",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/vuelib.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && yarn build:es && yarn build:js && yarn build:js_css",
    "build:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "build:js": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build:js_css": "cross-env NODE_ENV=production SEP_CSS=true rollup --config rollup.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-url": "^6.0.0",
    "@vue/compiler-sfc": "3.0.0",
    "autoprefixer": "9.7.5",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.3.2",
    "postcss-import": "^14.0.2",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^6.1.0",
    "postcss-nested": "^5.0.5",
    "postcss-simple-vars": "^6.0.3",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.34.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "vue": "3.1.1"
  },
  "peerDependencies": {
    "vue": ">=3"
  }
}
